<div class="panel">
  <%= form_for(@coordinator,html: { multipart: true }) do |f| %>
  <% if @coordinator.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@coordinator.errors.count, "error") %> prohibited this coordinator from being saved:</h2>

      <ul>
      <% @coordinator.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :nombres %>
    <%= f.text_field :nombres,{class:'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.label :apellidos %>
    <%= f.text_field :apellidos ,{class:'form-control'}%>
  </div>
  <div class="form-group">
    <%= f.label :curriculum %>
    <%= f.file_field :curriculum ,{class:'form-control'}%>
  </div>
    <%= f.label :visto %>
  <div class="form-group" ">
    <%= f.check_box :visto %>
  </div>
    <%= f.label :estado %>
  <div class="form-group">
    <%= f.check_box :estado ,{:data => { :'on-text' => 'Activo',:'off-text' => 'Inactivo' },class:'check-switch'} %>
  </div>
</div>

  <div class="form-group">
    <%= f.submit 'Guardar',{class:'btn btn-success'}%>
  </div>
<% end %>
