<h1>Este es el login de admin</h1>

<% if flash[:errores] %>
<%=flash[:errores] %>
<%end%>

<%= form_for(:session_admin,url:admin_login_path) do |f|%>
<%= f.text_field :username , placeholder: 'Username' %></br>
<%= f.password_field :password, placeholder: 'Password' %></br>
<%= f.submit 'Iniciar session' %>
<%end%>