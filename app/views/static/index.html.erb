				</div>
			</div>
			<div class="row">
			<%@cursos.uniq.each do |curso|%>
					<div class="col-md-4 ">
						<div class="panel panel-default panel-profile m-b-0">
							<div class="panel-heading" style="background-image: url(<%=curso.imagen.url(:thumb)%>)">
								
							</div>
							<div class="panel-body text-center" style="position:relative;">
														
							
								<h5 class="panel-title titulo"><%=curso.titulo%></h5>
								<%if curso.programations.size>0%>
									<p>En <%=curso.programations.size%> Instituciones</p>
								<%elsif%>
									<h6>Pronto disponible</h6>
								<%end%>
								<p class="m-b"></p>
								<a href="/curso/<%=curso.id%>" class="btn btn-info btn-primary-outline btn-sm m-b">
									<span class="icon icon-add-user"></span>
									Ver curso
								</a>
							
						</div>
					</div>
				</div>
			<%end%>

			<div class="col-md-12 text-center ">
