	<div id="verify-section" style="background:brown;" class="animate-box ">
		<div class="container text-center" >
		<div class="col-xs-10 col-md-8 col-xs-offset-1 col-md-offset-2" style="padding:3em 0;">
			<h1 style="font-weight:bold;color:#fff;" class="m-b">VERIFICA TU MATRICULA</h1>
			<h4 style="color:#fff;" class="m-b-lg">Ingresa tu nombre de usuario</h4>
			<%=form_tag verificar_url,{:"data-parsley-validate"=>'true',method:'get'} do%>
			<div class="row row-personalizado " >
				
					<div class="col-xs-12 col-sm-9 m-b-hasta-sm "
					 style="padding:0">
						<%=text_field_tag 'username','',{class:'form-control input-sin-outline',style:'padding:1em .1em;font-size:30px','data-parsley-length':'[5,12]',required:true}%>
					</div>
					<div class="col-xs-12 col-sm-3 " style="padding:none;">
						<%=submit_tag 'VERIFICAR',{class:'btn btn-warning  btn-block',style:'padding:1em 0'}%>
					</div>
			</div>
				<%end%>
		</div>
		</div>
	</div>
	<div id="fh5co-services-section" class="animate-box quitar-padding">
		<div class="container">
			<div class="row">
