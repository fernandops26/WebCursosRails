						<div class="services" >
							<div style="width:57px;display:inline-block;position:absolute;top:20px;box-sizing:border-box;">
							<%= image_tag cat.imagen.url(:mini),class:'img img-thumbnail' %>
							</div>
							<div class="desc" style="display:inline-block">
								<h3><%=cat.nombre%></h3>
								<p class="romper-linea"><%=cat.descripcion%></p>
							</div>
						</div>
					</div>
				<%end%>
				
			</div>
		</div>
	</div>
